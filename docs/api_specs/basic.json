                        "items": {
                            "type": "array",
                            "items": {
                                "type": "boolean"
                            }
                        }
                    },
                    "weights": {
                        "type": "array",
                        "description": "Contains the weights assigned to each feature in the model.",
                        "items": {
                            "type": "array",
                            "items": {
                                "type": "number",
                                "format": "float"
                            }
